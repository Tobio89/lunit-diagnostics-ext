{
  "short_name": "Scope Diagnostics",
  "name": "Lunit SCOPE Diagnostics",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "externally_connectable": {
    "matches": ["http://localhost:3000/*"]
  },
  "background": {
    "service_worker": "background.js"
  }
}
